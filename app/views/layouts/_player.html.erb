<div id="jquery_jplayer_1" class="jp-jplayer"></div>
<div id="jp_container_1" class="jp-audio" role="application" aria-label="media player">
  <div class="jp-type-playlist">
    <div class="jp-gui jp-interface">
      <div class="jp-controls btn-group">
        <button class="jp-previous btn btn-inverse" role="button" tabindex="0"><i class="fa fa-fast-backward"></i></button>
        <button class="jp-play btn btn-inverse" role="button" tabindex="0"><i class="fa fa-play"></i></button>
        <button class="jp-pause btn btn-inverse" role="button" tabindex="0"><i class="fa fa-pause"></i></button>
        <button class="jp-next btn btn-inverse" role="button" tabindex="0"><i class="fa fa-fast-forward"></i></button>
        <button class="jp-stop btn btn-inverse" role="button" tabindex="0"><i class="fa fa-stop"></i></button>
      </div>
      <div class="jp-progress">
        <div class="jp-seek-bar">
          <div class="jp-play-bar"></div>
        </div>
      </div>
      <div class="jp-volume-controls">
        <button class="jp-mute" role="button" tabindex="1">mute</button>
        <button class="jp-volume-max" role="button" tabindex="0">max volume</button>
        <div class="jp-volume-bar">
          <div class="jp-volume-bar-value"></div>
        </div>
      </div>
      <div class="jp-time-holder">
        <div class="jp-current-time" role="timer" aria-label="time">&nbsp;</div>
        <div class="jp-duration" role="timer" aria-label="duration">&nbsp;</div>
      </div>
      <div class="jp-toggles">
        <button class="jp-repeat" role="button" tabindex="0">repeat</button>
        <button class="jp-shuffle" role="button" tabindex="0">shuffle</button>
      </div>
    </div>
    <div class="jp-playlist">
      <ul>
        <li>&nbsp;</li>
      </ul>
    </div>
    <div>
      <div ng-repeat="track in nowPlayingList track by $index">
        <em ng-click="remove($index)">
          (-)
        </em>
        {{track.title}}
      </div>
    </div>
    <div class="jp-no-solution">
      <span>Update Required</span>
      jPlayer-no-solution message: perhaps no support for this filetype or browser... use chrome/chromium.
    </div>
  </div>
</div>
